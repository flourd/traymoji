{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2015",
    "module": "esnext",
    "moduleResolution": "node",
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@gitmoji": ["./src/Gitmoji"],
      "@gitmoji/*": ["./src/Gitmoji/*"],
      "@types": ["./src/types"],
      "@types/*": ["./src/types/*", "./src/types/*.d.ts"],
      "@/*": ["./src/*"]
    }
  }
}
